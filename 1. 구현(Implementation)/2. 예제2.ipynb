   "source": [
    "# N 입력받기\n",
    "n = int(input())\n",
    "\n",
    "# 3이 하나라도 포함되어 있는 시간의 갯수\n",
    "total = 0\n",
    "\n",
    "for h in range(n+1):\n",
    "    for m in range(60):\n",
    "        for s in range(60):\n",
    "            time = str(h).zfill(2) + str(m).zfill(2) + str(s).zfill(2) # zfill 안 해도 가능\n",
    "            total += int('3' in time)\n",
    "\n",
    "print(total)"
   ]
  }
 ],
