   "source": [
    "n = input()\n",
    "\n",
    "number = len(n) // 2\n",
    "\n",
    "left_value = 0\n",
    "for i in range(number) :\n",
    "  left_value += int(n[i])\n",
    "\n",
    "right_value = 0\n",
    "for i in range(number, len(n)) :\n",
    "  right_value += int(n[i])\n",
    "\n",
    "if left_value == right_value :\n",
    "  print('LUCKY')\n",
    "else :\n",
    "  print('READY')"
   ]
  }
