   "source": [
    "n = list(map(int, input()))\n",
    "\n",
    "result = n[0]\n",
    "\n",
    "for i in range(1, len(n)):   \n",
    "    if n[i] <= 1 or result <= 1:            \n",
    "        result += n[i]\n",
    "    else :                      \n",
    "        result *= n[i]\n",
    "        \n",
    "print(result)"
   ]
