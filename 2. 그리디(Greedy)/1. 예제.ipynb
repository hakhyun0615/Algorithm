   "source": [
    "# 금액 입력받기\n",
    "price = int(input())\n",
    "\n",
    "# 사용가능한 동전 종류\n",
    "coin_type = [500, 100, 50, 10]\n",
    "\n",
    "# 거슬러 줘야할 동전의 최소 개수\n",
    "coin_total = 0\n",
    "\n",
    "for coin in coin_type:\n",
    "    coin_total += price // coin\n",
    "    price %= coin\n",
    "\n",
    "print(coin_total)"
   ]
