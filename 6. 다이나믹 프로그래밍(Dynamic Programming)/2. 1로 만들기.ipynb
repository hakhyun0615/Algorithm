    "x = int(input())\n",
    "\n",
    "d = [0] * 30001\n",
    "\n",
    "for i in range(2,x+1):\n",
    "    d[i] = d[i-1]+1 \n",
    "    if i%2 == 0:\n",
    "        d[i] = min(d[i], d[i//2]+1)\n",
    "    elif i%3 == 0:\n",
    "        d[i] = min(d[i], d[i//3]+1)\n",
    "    elif i%5 == 0:\n",
    "        d[i] = min(d[i], d[i//5]+1)\n",
    "\n",
    "print(d[x])"
   ]
