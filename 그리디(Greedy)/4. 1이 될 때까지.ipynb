   "source": [
    "# N,K 입력받기\n",
    "n,k = map(int, input().split())\n",
    "\n",
    "# N이 1이 될 때까지 최소 횟수\n",
    "total = 0\n",
    "\n",
    "while n != 1:\n",
    "    if n % k == 0:\n",
    "        n //= k\n",
    "        total += 1\n",
    "    else:\n",
    "        n -= 1\n",
    "        total += 1\n",
    "\n",
    "print(total)"
   ]
  }
 ],
