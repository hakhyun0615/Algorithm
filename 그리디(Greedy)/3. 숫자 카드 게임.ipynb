    "# N,M 입력받기\n",
    "n,m = map(int, input().split())\n",
    "\n",
    "# 가장 큰 수\n",
    "max_num = 0\n",
    "\n",
    "for i in range(n):\n",
    "    row = list(map(int, input().split()))\n",
    "    row_min_num = min(row)\n",
    "    max_num = max(max_num, row_min_num)\n",
    "\n",
    "print(max_num)"
   ]
