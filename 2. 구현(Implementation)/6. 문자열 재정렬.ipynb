   "source": [
    "s = input()\n",
    "\n",
    "number = [str(n) for n in range(1, 10)]\n",
    "sum = 0\n",
    "result = []\n",
    "for i in s:\n",
    "    if i in number:\n",
    "        sum += int(i)\n",
    "    else:\n",
    "        result.append(i)\n",
    "\n",
    "result = sorted(result)\n",
    "\n",
    "if sum > 0:\n",
    "    result.append(str(sum))\n",
    "    \n",
    "print(\"\".join(result))"
   ]
  }
