   "source": [
    "# 모험가 수\n",
    "n = int(input(()))\n",
    "# 공포도 리스트\n",
    "l = list(map(int, input().split()))\n",
    "# 오름차순 정렬\n",
    "l.sort()\n",
    "\n",
    "# 그룹 수\n",
    "g = 0\n",
    "# 현재 멤버 수\n",
    "m = 0\n",
    "\n",
    "for f in l:\n",
    "    # 현 멤버 수 1 추가\n",
    "    m += 1\n",
    "    # 공포도 <= 현 멤버 수라면\n",
    "    if f <= m: \n",
    "        # 그룹 수 1 추가\n",
    "        g += 1\n",
    "        # 현 멤버 수 0 초기화\n",
    "        m = 0\n",
    "\n",
    "print(g)"
   ]
  }
 ],
